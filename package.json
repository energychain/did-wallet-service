{
  "name": "did-wallet-service",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect TCP",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down",
    "build": "node_modules/.bin/browserify lib/WebClient.js -s WebClient > public/webclient/client.js"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "browserify": "^17.0.0",
    "jest": "^27.1.0",
    "jest-cli": "^27.1.0",
    "microbundle": "^0.14.2",
    "moleculer-repl": "^0.6.6"
  },
  "dependencies": {
    "ajv": "^8.9.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.24.0",
    "cloudwallet": "^0.5.4",
    "did-resolver": "^3.1.5",
    "did-wallet-web": "^0.5.0",
    "eth-crypto": "^2.1.0",
    "ethers": "^5.5.3",
    "ethr-did": "^2.2.0",
    "ethr-did-resolver": "^5.0.3",
    "ipfs": "^0.61.0",
    "jsontokens": "^3.0.0",
    "moleculer": "^0.14.16",
    "moleculer-db": "^0.8.17",
    "moleculer-web": "^0.10.2",
    "orbit-db": "^0.28.0"
  },
  "engines": {
    "node": ">= 12.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
